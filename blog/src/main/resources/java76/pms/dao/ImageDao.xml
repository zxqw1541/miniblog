<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="java76.pms.dao.ImageDao">

  
  
  <select id="selectList" parameterType="int">
    select ano, path from image where ano='#{ano}'   
  </select>

  <insert id="insert" parameterType="image">
    insert into image(ano, path) 
    values(#{ano},#{path})
  </insert>
</mapper>






